<ul>
  {% for site in sites %}
    <li>
      <a href="{{ url('sso/goto/' ~ site.id, {'query':{'destination': 'node/' ~ node.id}}) }}">
        {{ site.title }}
      </a>
        {% if origin and site.id == origin.id %}
          ({{ "Origin"|trans }})
        {% endif %}
    </li>
  {% endfor %}
</ul>
